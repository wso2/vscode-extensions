{
  "editor.formatOnSave": false,
  "search.followSymlinks": false,
  "typescript.tsc.autoDetect": "off",
  "prettier.printWidth": 120,
  "prettier.tabWidth": 4,
  "files.associations": {
    "rush.json": "jsonc"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".mule": true
  },
}
